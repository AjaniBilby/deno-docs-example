{
	"editor.insertSpaces": false,
	"cSpell.words": [	],
	"files.eol": "\n",
	"editor.rulers": [ 80, 120 ],

	// "editor.formatOnSave": true,
	"[typescript]": {
		"editor.defaultFormatter": "denoland.vscode-deno"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "denoland.vscode-deno"
	},
	"[javascript]": {
		"editor.defaultFormatter": "denoland.vscode-deno"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "denoland.vscode-deno"
	},

	"deno.enable": true,
	"deno.lint": true,
	"deno.suggest.imports.hosts": {
		"https://deno.land": true
	}
}